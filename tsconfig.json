{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/HistoryCard.jsx",
    "components/SelectInput.jsx",
    "app/(tabs)/bin.jsx",
    "components/dateTimePicker.jsx",
    "components/pickupSheet.jsx"
  ]
}
